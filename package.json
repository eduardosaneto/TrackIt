{
  "name": "trackit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-confirm-alert": "^2.7.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "styled-icons": "^10.33.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "npx prettier --write --ignore-unknown .",
    "eslint": "npx eslint src",
    "eslint:fix": "npx eslint --fix src --ext .js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "react-hooks/rules-of-hooks": "off",
      "indent": [
        "error",
        2
      ],
      "linebreak-style": 0,
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "semi-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false
        }
      ],
      "padded-blocks": [
        "error",
        "never"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1
        }
      ],
      "lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "space-before-blocks": [
        "error",
        "always"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "arrow-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "git-commit-msg-linter": "^3.2.8",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1"
  }
}
